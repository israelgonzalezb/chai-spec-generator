{
  "name": "chai-spec-generator",
  "displayName": "chai-spec-generator",
  "description": "Generates spec assertion code for a selected object or array",
  "version": "0.3.2",
  "publisher": "rintoj",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rintoj/chai-spec-generator.git"
  },
  "activationEvents": [
    "onCommand:extension.generate-test-spec"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.generate-test-spec",
        "title": "Generate Test Spec"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^1.8.5",
    "vscode": "^0.11.0"
  }
}
